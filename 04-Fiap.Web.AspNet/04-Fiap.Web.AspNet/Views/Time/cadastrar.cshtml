@model Time

@{
    ViewData["Title"] = "cadastrar";
}

<h1 class="">Cadastrar</h1>

<div class="form-row">
    <form asp-action="Cadastrar" asp-controller="Time">
        <div class="form-group">
            <label asp-for="Nome"></label>
            <input asp-for="Nome" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="DataFundacao"></label>
            <input asp-for="DataFundacao" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="FolhaPagamento"></label>
            <input asp-for="FolhaPagamento" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Classificado" class="form-check-label"></label>
            <input asp-for="Classificado" class="custom-checkbox form-check" />
        </div>
        <div class="form-group">
            <label asp-for="Vitorias"></label>
            <input asp-for="Vitorias" class="form-control" />
        </div>

        <input type="submit" class="btn btn-outline-dark" value="OK" />
        <a asp-controller="Time" asp-action="Listar" class="btn btn-outline-dark">Listar</a>

    </form>
</div>



